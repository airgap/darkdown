    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "parser": "babel-ts"
  },
  "devDependencies": {
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "ts-node": "^10.2.1"
  }
}
