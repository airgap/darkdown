{
  "name": "darkdown",
  "version": "0.0.0",
  "description": "If markdown was a programming language",
  "main": "index.js",
  "scripts": {
    "start": "ts-node compileToJs.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Nicole Muzzin",
  "license": "UNLICENSED",
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
